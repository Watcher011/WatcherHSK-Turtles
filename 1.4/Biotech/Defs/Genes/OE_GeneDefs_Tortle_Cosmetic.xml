<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!--=============== Skin - TurtleDarkGreen ====================-->
  <GeneDef ParentName="GeneSkinColorOverride">
    <defName>OE_Skin_TurtleDarkGreen</defName>
    <label>turtle dark green skin</label>
    <labelShortAdj>turtle dark green</labelShortAdj>
    <description>Carriers of this gene produce a pigment that turns their skin a turtle dark green color.</description>
    <skinColorOverride>(77, 98, 79)</skinColorOverride>
    <displayOrderInCategory>708</displayOrderInCategory>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>green</symbol></li>
        <li><symbol>turtle</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>
